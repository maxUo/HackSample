<?xml version="1.0" encoding="UTF-8"?>
<pages:BasePage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="UnblockHackNET.UI.Pages.MainBurger.MenuPage"
    xmlns:pages="clr-namespace:UnblockHackNET.UI.Pages">
    <StackLayout
        Margin="10,20,10,0"
        VerticalOptions="Center"
        Spacing="20">
        <Button
            Text="Главная"
            Command="{Binding GoToMainCommand}">
        </Button>
        <Button
            Text="Настройки"
            Command="{Binding GoToSettingsCommand}">
        </Button>
        <Button
            Text="Выйти">
        </Button>
    </StackLayout>
</pages:BasePage>